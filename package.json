{
  "name": "@webburrito/goatcurry",
  "version": "1.0.125",
  "description": "A Open Source Content Editor, still very much a work in progress.",
  "main": "./dist/index.bundle.js",
  "scripts": {
    "watch": "webpack --mode development",
    "start": "webpack-dev-server --open",
    "build": "npm run build:types && npm run build:js",
    "update-contrib": "weallcontribute -o . && git add CONTRIBUTING.md && git commit -m 'docs(contributing): updated CONTRIBUTING.md'",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build:types": "tsc",
    "build:js": "babel src --extensions \".ts,.tsx\" --source-maps inline"
  },
  "bugs": {
    "url": "https://github.com/tomkiernan120/goat-curry/issues",
    "email": "tkiernan120@gmail.com"
  },
  "author": "Tom Kiernan",
  "license": "GPL-3.0-only",
  "dependencies": {
    "@babel/polyfill": "^7.4.3",
    "@types/webpack-env": "^1.14.0",
    "extend": "^3.0.2",
    "json-beautify": "^1.1.0",
    "sanitize-html": "^1.20.0",
    "sizzle": "^2.3.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@fortawesome/fontawesome-free": "5.10.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "8.0.6",
    "css-loader": "3.2.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "13.2.0",
    "eslint-loader": "2.2.1",
    "eslint-plugin-import": "2.18.2",
    "jasmine": "3.4.0",
    "jshint": "2.10.2",
    "qunit": "2.9.2",
    "source-map-loader": "^0.2.4",
    "store": "2.0.12",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "weallcontribute": "1.0.9",
    "webpack": "4.39.1",
    "webpack-cli": "3.3.6",
    "webpack-dev-server": "3.7.2"
  },
  "renovate": {
    "automerge": true,
    "major": {
      "automerge": false
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tomkiernan120/Goat-Curry"
  }
}
